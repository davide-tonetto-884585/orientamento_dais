			"type": "Trigger",
			"code": "CREATE TRIGGER trigger_check_like\n    BEFORE INSERT OR UPDATE\n    ON public.like_domanda\n    FOR EACH ROW\n    EXECUTE FUNCTION public.check_like();",
			"lines": [],
			"generate": true,
			"generateCustomCode": true,
			"pg": {
